{
  "name": "ashtangamoon-solid",
  "version": "2.2.0",
  "description": "Allows to keep track of new and full moon days and visualizes current moon phase",
  "type": "module",
  "scripts": {
    "dev": "VITE_EMULATE=true PWA_DISABLE=true vite",
    "build": "tsc && vite build",
    "build:watch": "tsc && VITE_EMULATE=true PWA_DISABLE=true vite build --watch",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit && pnpm --prefix './functions' run typecheck",
    "format": "prettier --write .",
    "emulators": "pnpm run functions:build && GOOGLE_APPLICATION_CREDENTIALS='/home/kos/Desktop/projects/ashtangamoon-cbab049dea6e.json' firebase emulators:start",
    "functions:build": "pnpm --prefix './functions' run build",
    "functions:build:watch": "pnpm --prefix './functions' run build:watch",
    "deploy": "firebase deploy"
  },
  "dependencies": {
    "@ark-ui/solid": "^5.30.0",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "firebase": "^12.8.0",
    "lune": "^0.4.0",
    "solid-js": "^1.9.11",
    "solid-toast": "^0.5.0",
    "typesaurus": "^10.7.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/d3": "^7.4.3",
    "cssnano": "^7.1.2",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-minify": "^2.1.0",
    "vite-plugin-pwa": "^1.2.0",
    "vite-plugin-solid": "^2.11.10",
    "workbox-core": "^7.4.0",
    "workbox-precaching": "^7.4.0",
    "workbox-window": "^7.4.0"
  },
  "keywords": [
    "pwa",
    "pwa-apps",
    "progressive-web-app",
    "progressive-web-apps",
    "workbox",
    "firebase",
    "firebase-auth",
    "firebase-firestore",
    "firebase-cloud-functions",
    "firebase-cloud-messaging",
    "yoga",
    "ashtanga",
    "ashtanga-yoga"
